#include <iostream>
#include <cmath>      // for sqrt, round, floor, ceil, abs
#include <algorithm>  // for max, min
using namespace std;

int main() {
    cout << max(5, 10) << endl; //10
    cout << min(5, 10) << endl; //5
    cout << sqrt(64) << endl; //8
    cout << round(2.6) << endl; //3
    cout << floor(2.6) << endl; //2
    cout << ceil(5.6) << endl; //6
    cout << abs(-664) << endl; //664

    return 0;
}
