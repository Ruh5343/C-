#include <iostream>
#include <cmath>      // for sqrt, round, floor, ceil, abs
#include <algorithm>  // for max, min
using namespace std;

int main() {

    cout << round(-2.2) << endl; //-2

    cout << ceil(-2.2) << endl; //-2
  cout << round(-2.6) << endl; //-3

    cout << ceil(-2.6) << endl; //-2

    return 0;
}

